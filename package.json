{
  "name": "react-angler",
  "version": "0.1.0",
  "description": "Functional react hooks to solve everyday problems in functional components.",
  "sideEffects": false,
  "repository": "https://www.github.com/jovidecroock/React-angler",
  "keywords": [
    "hooks",
    "react",
    "functional"
  ],
  "scripts": {
    "build": "microbundle",
    "filesize": "bundlesize"
  },
  "main": "dist/react-angler.js",
  "browser": "dist/react-angler.umd.js",
  "module": "dist/react-angler.module.js",
  "typings": "lib/index.d.ts",
  "license": "MIT",
  "mangle": {
    "regex": "^_"
  },
  "devDependencies": {
    "@types/react": "16.7.20",
    "bundlesize": "0.17.1",
    "cross-env": "5.2.0",
    "microbundle": "0.9.0",
    "react": "16.8.0-alpha.1",
    "react-dom": "16.8.0-alpha.1",
    "tslib": "1.9.3",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-react": "3.6.0",
    "typescript": "3.1.3"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "16.8.0-alpha.1",
    "react-dom": "16.8.0-alpha.1"
  },
  "bundlesize": [
    {
      "path": "./dist/react-angler.module.js",
      "maxSize": "1.6 kB"
    },
    {
      "path": "./dist/react-angler.umd.js",
      "maxSize": "1.6 kB"
    },
    {
      "path": "./dist/react-angler.js",
      "maxSize": "1.5 kB"
    }
  ]
}
